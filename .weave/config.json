        "elasticsearch": {
            "display_name": "Elasticsearch",
            "description": "Search and analytics engine",
            "images": ["docker.elastic.co/elasticsearch/elasticsearch:8.10.4"],
            "container_patterns": ["elastic"]
        },
        "webcat": {
            "display_name": "Web Search",
