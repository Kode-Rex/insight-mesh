# RAG Pipeline Environment Configuration

# Haystack Search Configuration
HAYSTACK_ENDPOINT=http://haystack:8000/search
HAYSTACK_MAX_RESULTS=3

# Redis Cache Configuration 
REDIS_ENABLED=true
REDIS_URL=redis://redis:6379/0
REDIS_TTL=3600

# PostgreSQL Database Configuration for User Lookup
DB_HOST=postgres
DB_PORT=5432
DB_NAME=openwebui
DB_USER=postgres
DB_PASSWORD=postgres
DB_USER_TABLE=users

# JWT Configuration
# JWT_SECRET_KEY=your-secret-key
